{
  "name": "unplugin-swc",
  "version": "0.0.0",
  "packageManager": "pnpm@8.7.0",
  "description": "SWC plugin for Vite and Rollup",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "scripts": {
    "build": "tsup",
    "test": "npm run build && vitest",
    "prepublishOnly": "npm run build"
  },
  "license": "MIT",
  "dependencies": {
    "unplugin": "^0.6.0"
  },
  "devDependencies": {
    "@egoist/prettier-config": "1.0.0",
    "@rollup/pluginutils": "^4.1.1",
    "@swc/core": "^1.2.108",
    "@types/node": "^17.0.23",
    "defu": "^6.0.0",
    "esbuild": "0.14.9",
    "load-tsconfig": "^0.2.3",
    "path-exists": "^5.0.0",
    "prettier": "2.5.1",
    "rollup": "^2.60.0",
    "tsup": "7.2.0",
    "typescript": "4.5.4",
    "vite": "^2.6.14",
    "vitest": "^0.34.3"
  },
  "peerDependencies": {
    "@swc/core": "^1.2.108"
  }
}
